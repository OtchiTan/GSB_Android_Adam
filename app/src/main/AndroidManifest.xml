<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gsb">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GSB">
        <activity
            android:name=".MajEchantillon"
            android:exported="false"
            android:label="Mettre à jour un échantillon"
            android:parentActivityName=".Accueil" />
        <activity
            android:name=".ListeEchantillon"
            android:exported="false"
            android:label="Liste des échantillons"
            android:parentActivityName=".Accueil" />
        <activity
            android:name=".AjoutEchantillon"
            android:exported="false"
            android:label="Ajouter un échantillon"
            android:parentActivityName=".Accueil" />
        <activity
            android:name=".Accueil"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>